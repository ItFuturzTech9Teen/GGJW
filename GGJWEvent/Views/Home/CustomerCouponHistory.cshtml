
@{
    ViewBag.Title = "CustomerCouponHistory";
}

<script src="~/Js/GGJWEvemt/Services/HomeService.js"></script>
<script src="~/Js/GGJWEvemt/Controllers/CustomerCoupnWithExhibitorData.js"></script>
<script src="~/Js/table2excel.js"></script>
<div class="container-fluid">
    <!-- Breadcrumb-->
    <div class="row pt-2 pb-2">
        <div class="col-sm-12">
            <ol class="breadcrumb">
                <li class="breadcrumb-item"><a href="javaScript:void();">Customers</a></li>
                <li class="breadcrumb-item active" aria-current="page">Customer Coupons History</li>
            </ol>
        </div>
    </div>
    <!-- End Breadcrumb-->
    <div ng-controller="CustomerCoupnWithExhibitorDataController">
        <div class="row">
            <div class="col-12">
                <div class="card">
                    <div class="card-body">
                        <div class="row">
                            <div class="col-12">
                                <div class="row">
                                    <div class="col-md-4">
                                        <h4 class="card-title" style="font-size:20px;">Customer Coupons History</h4>
                                    </div>
                                    <div class="col-md-4" style="padding-bottom:15px;">
                                        <input type="text" id="" class="form-control form-control-sm" placeholder="Search Here" ng-model="searchText" />
                                    </div>
                                    <div class="col-md-4" align="right">
                                        <input type="button" value="Export to Excel" id="btnexport" class="btn btn-info btn-sm" ng-click="exportData()" />
                                    </div>
                                </div>
                                <div class="table-responsive " id="export">
                                    <table class="table table-hover table-striped table-bordered table2excel">
                                        <thead>
                                            <tr>
                                                <th>SrNo</th>
                                                <th>VisitorName</th>
                                                <th>Qty</th>
                                                <th>Action</th>
                                            </tr>
                                        </thead>
                                        <tbody>
                                            <tr ng-repeat="data in CustomerList | filter:searchText">
                                                <td>{{$index+1}}</td>
                                                <td>{{data.PersonName}}</td>
                                                <td>{{data.TotalQty}}</td>
                                                <td>
                                                    <button type="button" ng-click="GetExhibitorList(data.Id)" class="btn btn-info btn-sm " data-toggle="modal" data-target="#modal"><i class="fa fa-list-ol"></i></button>
                                                </td>
                                            </tr>
                                        </tbody>
                                    </table>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <div class="col-lg-4">
            <div class="modal fade" id="modal">
                <div class="modal-dialog modal-md">
                    <div class="modal-content">
                        <div class="modal-header">
                            <h5 class="modal-title">Exhibitor List</h5>
                            <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                                <span aria-hidden="true">&times;</span>
                            </button>
                        </div>
                        <div class="modal-body">
                            <div class="row">
                                <div class="col-12">
                                    <div class="row">
                                        <div class="col-md-8">
                                        </div>
                                    </div>
                                    <div class="table-responsive " id="export">
                                        <table class="table table-hover table-striped table-bordered">
                                            <thead>
                                                <tr>
                                                    <th>ExhibitorName</th>
                                                    <th>Qty</th>
                                                </tr>
                                            </thead>
                                            <tbody>
                                                <tr ng-repeat="e in ExhibitorList">
                                                    <td>{{e.PersonName}}</td>
                                                    <td>{{e.Qty}}</td>
                                                </tr>
                                            </tbody>
                                        </table>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
<script src="https://cdnjs.cloudflare.com/ajax/libs/FileSaver.js/1.3.8/FileSaver.js"></script>



