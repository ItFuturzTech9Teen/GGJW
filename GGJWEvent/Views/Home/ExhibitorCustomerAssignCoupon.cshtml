
@{
    ViewBag.Title = "ExhibitorCustomerAssignCoupon";
}
<script src="~/Js/GGJWEvemt/Services/HomeService.js"></script>
<script src="~/Js/GGJWEvemt/Controllers/AssignCouponCustomer.js"></script>
<div class="container-fluid">
    <!-- Breadcrumb-->
    <div class="row pt-2 pb-2">
        <div class="col-sm-9">
            <h4 class="page-title">Customer Coupon List</h4>
            <ol class="breadcrumb">
                <li class="breadcrumb-item"><a href="javaScript:void();">GGJWEvent</a></li>
                <li class="breadcrumb-item"><a href="javaScript:void();">Masters</a></li>
                <li class="breadcrumb-item active" aria-current="page">Customer Coupon List</li>
            </ol>
        </div>
    </div>
    <!-- End Breadcrumb-->
    <div ng-controller="AssignCustomerCouponController">
        <div class="row">
            <div class="col-12">
                <div class="card">
                    <div class="card-body">
                        <div class="row">
                            <div class="col-12">
                                <div class="row">
                                    <div class="col-md-8">
                                        <h4 class="card-title" style="font-size:20px;">DATA</h4>
                                    </div>
                                </div>
                                <div class="table-responsive ">
                                    <table datatable="ng" dt-options="vm.dtOptions" class="table table-hover table-striped table-bordered">
                                        <thead>
                                            <tr>
                                                <th>SrNo</th>
                                                <th>CouponNo</th>
                                                <th>Customer Name</th>
                                                <th>MobileNo</th>
                                                <th>Price</th>
                                                <th>Qty</th>
                                                <th>ExhibitorName</th>
                                            </tr>
                                        </thead>
                                        <tbody>
                                            <tr ng-repeat="data in AssignCouponCustomerList">
                                                <td>{{$index+1}}</td>
                                                <td>{{data.CouponNo}}</td>
                                                <td>{{data.PersonName}}</td>
                                                <td>{{data.MobileNo}}</td>
                                                <td>{{data.Price}}</td>
                                                <td>{{data.Qty}}</td>
                                                <td>{{data.ExhibitorName}}</td>
                                            </tr>
                                        </tbody>
                                    </table>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>


